Dim UserInput
UserInput = InputBox("Skriv tal ind højere end 0: ")
Newnumber = CInt(UserInput)
Dim fso
Set fso = WScript.CreateObject("Scripting.FileSystemObject")
Dim fw
Set fw = fso.OpenTextFile("C:\Users\ADV-DBT\Dropbox\Skole\Hovedforløb 1\Programmering\Scripts\Output Files\Opgave 4 Output.txt", 2)
Dim Counter: Counter = 0
While Counter < Newnumber
	fw.WriteLine "Loop: " & Counter
	Counter = Counter + 1
Wend
fw.WriteLine ("Done!")
fw.Close
