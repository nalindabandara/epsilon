var client = Ti.Network.createHTTPClient({
            onload : function(e) {
                Data = JSON.parse(this.responseText);
                Size = Object.keys(Data).length;
                AddList();
            },
            onerror : function(e) {
            },
            timeout : 15000
        });
        client.open("GET", http://lamadeus.virtualweb.pt/site/app_mobile/teste.php?act=getprodsdestaque);
        client.send();
